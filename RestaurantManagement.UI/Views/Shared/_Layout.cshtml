
<!DOCTYPE html>
<html lang="zxx">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Foodio</title>
    <link rel="icon" href="~/site/assets/img/logo-icon.png">
    <link rel="stylesheet" type="text/css" href="~/site/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/site/assets/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/site/assets/css/style.css">
    <link rel="stylesheet" href="~/site/assets/css/responsive.css">
    <link rel="stylesheet" href="~/site/assets/css/color.css">
    <link href="~/lib/toaster/naranja.min.css" rel="stylesheet" />
    <link href="~/site/assets/css/main.css" rel="stylesheet" />
    <script src="~/site/assets/js/jquery-3.6.0.min.js"></script>
    <script src="~/site/assets/js/preloader.js"></script>
</head>
<body>
    <!-- end preloader -->
    <partial name="~/Views/Shared/_Header.cshtml" />
    <section class="gap">
        <div class="container">
            @RenderBody()
        </div>
    </section>
    <!-- footer -->
    <footer>
        <div class="container">
            <div class="footer-bootem">
                <h6><span>© 2023 Foodio</span></h6>
            </div>
        </div>
    </footer>
    <!-- progress -->
    <div id="progress">
        <span id="progress-value"><i class="fa-solid fa-arrow-up"></i></span>
    </div>
    <!-- fancybox -->
    <script src="~/site/assets/js/custom.js"></script>
    <script src="~/lib/toaster/naranja.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
